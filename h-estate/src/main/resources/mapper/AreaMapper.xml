<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.house.estate.mapper.AreaMapper">
    <select id="executeForObjectList" resultType="com.house.common.entity.estate.AreaCascader">
            select
                t.id as value,
                t.area_name as label,
                t.parent_id as parentId

            FROM
                area t

    </select>
    

</mapper>